[user]
	name = iSpeller
	email = shell_way@foxmail.com
[core]
	editor = vim
[color]
  ui = true
  pager = true
  branch = auto
  diff = auto
[color "status"]
  added = green
  changed = cyan
  untracked = yellow
[color "diff"]
  meta = green bold
  frag = magenta bold
  old = yellow
  new = cyan
[alias]
	bc = branch
	st = status
	co = checkout
	cm = commit
	df = diff
  rs = reset
  ps = push
  pl = pull
[push]
	default = simple
[merge]
	tool = vimdiff

